@using TicketingSystem.ViewModels
@model AppUserViewModel

@{
    ViewData["Title"] = $"{Model.NormalizedUserName} User Details";
}

<h1>@ViewData["Title"]</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Db User Name
        </dt>
        <dd class="col-sm-10">
            @Model.UserName
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd>
        <dt class="col-sm-2">
            Phone Number
        </dt>
        <dd class="col-sm-10">
            @Model.PhoneNumber
        </dd>
        <dt class="col-sm-2">
            Is Active
        </dt>
        <dd class="col-sm-10">
            @Model.IsActive
        </dd>
        <dt class="col-sm-2">
            Create Date
        </dt>
        <dd class="col-sm-10">
            @Model.CreateDate
        </dd>
        <dt class="col-sm-2">
            Last Update Date
        </dt>
        <dd class="col-sm-10">
            @Model.UpdateDate
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-outline-primary">Edit</a>
    <a asp-action="Index" class="btn btn-outline-primary">Back to List</a>
</div>
